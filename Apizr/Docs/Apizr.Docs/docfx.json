{
  "metadata": [
    {
      "src": [
        {
          "src": "../../",
          "files": [
            "Apizr/Apizr.csproj",
            "Apizr.Extensions.Microsoft.Caching/Apizr.Extensions.Microsoft.Caching.csproj",
            "Apizr.Extensions.Microsoft.DependencyInjection/Apizr.Extensions.Microsoft.DependencyInjection.csproj",
            "Apizr.Integrations.Akavache/Apizr.Integrations.Akavache.csproj",
            "Apizr.Integrations.AutoMapper/Apizr.Integrations.AutoMapper.csproj",
            "Apizr.Integrations.Fusillade/Apizr.Integrations.Fusillade.csproj",
            "Apizr.Integrations.MediatR/Apizr.Integrations.MediatR.csproj",
            "Apizr.Integrations.MonkeyCache/Apizr.Integrations.MonkeyCache.csproj",
            "Apizr.Integrations.Optional/Apizr.Integrations.Optional.csproj",
            "Apizr.Integrations.FileTransfer/Apizr.Integrations.FileTransfer.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "api/*.md",
          "articles/**",
          "index.md",
          "toc.yml",
          "changelog.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [ "resources/**" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Apizr",
      "_appFooter": "Supported by Respawnsive",
      "_appLogoPath": "resources/logo.png",
      "_appFaviconPath": "resources/favicon.ico",
      "_enableSearch": true,
      "_disableToc": false,
      "_disableContribution": false
    },
    "dest": "../../../docs",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}